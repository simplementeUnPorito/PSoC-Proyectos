<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>CyBle_HrsRegisterAttrCallback</Title>
			<Shortcut>CyBle_HrsRegisterAttrCallback</Shortcut>
			<Description>BLE HRS register callback function</Description>
			<Author>Cypress Semiconductor</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>CyBle_HrsRegisterAttrCallback</Default>
					<ToolTip>Function name</ToolTip>
				</Literal>
				<Literal>
					<ID>callback</ID>
					<Default>CyBle_HrsCallBack</Default>
					<ToolTip>BLE HRS CallBack function name</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="c">
<![CDATA[$name$( $callback$ );]]>
			</Code>
		</Snippet>
	</CodeSnippet>
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>CyBle_HrsCallBack</Title>
			<Shortcut>CyBle_HrsCallBack</Shortcut>
			<Description>BLE CallBack function for Heart Rate service</Description>
			<Author>Cypress Semiconductor</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>CyBle_HrsCallBack</Default>
					<ToolTip>Function Name</ToolTip>
				</Literal>
				<Literal>
					<ID>eventCode</ID>
					<Default>eventCode</Default>
					<ToolTip>HRS Event Code</ToolTip>
				</Literal>
				<Literal>
					<ID>eventParam</ID>
					<Default>eventParam</Default>
					<ToolTip>HRS Event Parameters</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="c">
<![CDATA[void $name$( uint32 $eventCode$, void *$eventParam$ )
{
    switch($eventCode$)
    {
    #ifdef CYBLE_HRS_SERVER
        case CYBLE_EVT_HRSS_ENERGY_EXPENDED_RESET:
            break;

        case CYBLE_EVT_HRSS_NOTIFICATION_ENABLED:
            break;

        case CYBLE_EVT_HRSS_NOTIFICATION_DISABLED:
            break;
    #endif /* CYBLE_HRS_SERVER */

    #ifdef CYBLE_HRS_CLIENT
        case CYBLE_EVT_HRSC_NOTIFICATION:
            break;

        case CYBLE_EVT_HRSC_READ_CHAR_RESPONSE:
            break;

        case CYBLE_EVT_HRSC_WRITE_CHAR_RESPONSE:
            break;

        case CYBLE_EVT_HRSC_READ_DESCR_RESPONSE:
            break;

        case CYBLE_EVT_HRSC_WRITE_DESCR_RESPONSE:
            break;
    #endif /* CYBLE_HRS_CLIENT */
    }
}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
