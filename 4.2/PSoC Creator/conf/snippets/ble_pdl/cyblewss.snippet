<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Cy_BLE_WSS_RegisterAttrCallback</Title>
			<Shortcut>Cy_BLE_WSS_RegisterAttrCallback</Shortcut>
			<Description>BLE WSS register callback function</Description>
			<Author>Cypress Semiconductor</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>Cy_BLE_WSS_RegisterAttrCallback</Default>
					<ToolTip>Function name</ToolTip>
				</Literal>
				<Literal>
					<ID>callback</ID>
					<Default>Cy_BLE_WSS_CallBack</Default>
					<ToolTip>BLE WSS CallBack function name</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="c">
<![CDATA[$name$( $callback$ );]]>
			</Code>
		</Snippet>
	</CodeSnippet>
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Cy_BLE_WSS_CallBack</Title>
			<Shortcut>Cy_BLE_WSS_CallBack</Shortcut>
			<Description>BLE CallBack function for Weight Scale service</Description>
			<Author>Cypress Semiconductor</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>Cy_BLE_WSS_CallBack</Default>
					<ToolTip>Function Name</ToolTip>
				</Literal>
				<Literal>
					<ID>eventCode</ID>
					<Default>eventCode</Default>
					<ToolTip>WSS Event Code</ToolTip>
				</Literal>
				<Literal>
					<ID>eventParam</ID>
					<Default>eventParam</Default>
					<ToolTip>WSS Event Parameters</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="c">
<![CDATA[void $name$( uint32 $eventCode$, void *$eventParam$ )
{
    switch($eventCode$)
    {
    #ifdef CY_BLE_WSS_SERVER
        case CY_BLE_EVT_WSSS_INDICATION_ENABLED:
            break;

        case CY_BLE_EVT_WSSS_INDICATION_DISABLED:
            break;

        case CY_BLE_EVT_WSSS_INDICATION_CONFIRMED:
            break;
    #endif /* CY_BLE_WSS_SERVER */

    #ifdef CY_BLE_WSS_CLIENT
        case CY_BLE_EVT_WSSC_INDICATION:
            break;

        case CY_BLE_EVT_WSSC_READ_CHAR_RESPONSE:
            break;

        case CY_BLE_EVT_WSSC_READ_DESCR_RESPONSE:
            break;

        case CY_BLE_EVT_WSSC_WRITE_DESCR_RESPONSE:
            break;
    #endif /* CY_BLE_WSS_CLIENT */
    }
}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
