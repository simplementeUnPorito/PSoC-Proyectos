<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Cy_BLE_HIDS_RegisterAttrCallback</Title>
			<Shortcut>Cy_BLE_HIDS_RegisterAttrCallback</Shortcut>
			<Description>BLE HIDS register callback function</Description>
			<Author>Cypress Semiconductor</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>Cy_BLE_HIDS_RegisterAttrCallback</Default>
					<ToolTip>Function name</ToolTip>
				</Literal>
				<Literal>
					<ID>callback</ID>
					<Default>Cy_BLE_HIDS_CallBack</Default>
					<ToolTip>BLE HIDS CallBack function name</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="c">
<![CDATA[$name$( $callback$ );]]>
			</Code>
		</Snippet>
	</CodeSnippet>
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Cy_BLE_HIDS_CallBack</Title>
			<Shortcut>Cy_BLE_HIDS_CallBack</Shortcut>
			<Description>BLE CallBack function for HID service</Description>
			<Author>Cypress Semiconductor</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<Default>Cy_BLE_HIDS_CallBack</Default>
					<ToolTip>Function Name</ToolTip>
				</Literal>
				<Literal>
					<ID>eventCode</ID>
					<Default>eventCode</Default>
					<ToolTip>HIDS Event Code</ToolTip>
				</Literal>
				<Literal>
					<ID>eventParam</ID>
					<Default>eventParam</Default>
					<ToolTip>HIDS Event Parameters</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="c">
<![CDATA[void $name$( uint32 $eventCode$, void *$eventParam$ )
{
    switch($eventCode$)
    {
    #ifdef CY_BLE_HIDS_SERVER
        case CY_BLE_EVT_HIDSS_NOTIFICATION_ENABLED:
            break;

        case CY_BLE_EVT_HIDSS_NOTIFICATION_DISABLED:
            break;

        case CY_BLE_EVT_HIDSS_BOOT_MODE_ENTER:
            break;

        case CY_BLE_EVT_HIDSS_REPORT_MODE_ENTER:
            break;

        case CY_BLE_EVT_HIDSS_SUSPEND:
            break;

        case CY_BLE_EVT_HIDSS_EXIT_SUSPEND:
            break;

        case CY_BLE_EVT_HIDSS_REPORT_WRITE_CHAR:
            break;
    #endif /* CY_BLE_HIDS_SERVER */

    #ifdef CY_BLE_HIDS_CLIENT
        case CY_BLE_EVT_HIDSC_NOTIFICATION:
            break;

        case CY_BLE_EVT_HIDSC_READ_CHAR_RESPONSE:
            break;

        case CY_BLE_EVT_HIDSC_WRITE_CHAR_RESPONSE:
            break;

        case CY_BLE_EVT_HIDSC_READ_DESCR_RESPONSE:
            break;

        case CY_BLE_EVT_HIDSC_WRITE_DESCR_RESPONSE:
            break;
    #endif /* CY_BLE_HIDS_CLIENT */
    }
}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
